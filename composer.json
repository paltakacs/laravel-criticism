{
  "name": "paltakacs/laravel-criticism",
  "description": "Add comments to your Laravel app",
  "keywords": [
    "paltakacs",
    "laravel-criticism"
  ],
  "homepage": "https://github.com/paltakacs/laravel-criticism",
  "license": "MIT",
  "authors": [
    {
      "name": "Pal Takacs",
      "email": "pal@limewebdevelopment.com",
      "homepage": "https://limewebdevelopment.com/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2",
    "laravel/framework": "~5.8|^6.0|^7.0",
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.0",
  },
  "autoload": {
    "psr-4": {
      "PalTakacs\\Criticism\\": "src"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "PalTakacs\\Criticism\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "PalTakacs\\Criticism\\CriticismServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
